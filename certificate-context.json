{
    "@context": [
        {
            "@version": 1.1
        },
        "https://www.w3.org/ns/odrl.jsonld",
        {
            "institute": "xsd:string",
            "documentName": "xsd:string",
            "issueDateAndTime": "xsd:string",
            "certificateId": "xsd:string",
            "recipientDetails": {
                "@id": "recipientDetails",
                "@context": {
                  "fname": "xsd:string",
                  "lname": "xsd:string",
                  "fnameEN": "xsd:string",
                  "lnameEN": "xsd:string",
                  "sex": "xsd:string",
                  "nationality": "xsd:string",
                  "dateOfBirth": "xsd:string",
                  "placeOfBirth": "xsd:string"
                }
            },
            "certificateDetails": {
                "@id": "certificateDetails",
                "@context": {
                  "level": "xsd:string",
                  "major": "xsd:string",
                  "dateOfCompletion": "xsd:string"
                }
            },
            "links": {
                "@id": "https://schemata.openattestation.com/vocab/#links",
                "@context": {
                "self": {
                    "@id": "https://schemata.openattestation.com/vocab/#self",
                    "@context": {
                    "href": "xsd:string"
                    }
                }
                }
            }     
        }     
    ]
}